{
  "expo": {
    "name": "SocialZ",
    "slug": "socialz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo7.svg",
    "scheme": "socialz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ]
      },
      "bundleIdentifier": "com.r.tomson.socialz",
      "icon": "./assets/images/logo7.svg"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo7.svg",
        "backgroundColor": "#000000"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.NOTIFICATIONS",
        "android.permission.RECORD_AUDIO",
        "android.permission.NOTIFICATIONS",
        "android.permission.RECORD_AUDIO"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.student.app",
      "icon": "./assets/images/logo7.svg"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo7.svg"
    },
    "splash": {
      "image": "./assets/images/logo7.svg",
      "resizeMode": "contain",
      "imageWidth": 200,
      "imageHeight": 200,
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo7.svg",
          "imageResizeMode": "contain",
        
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you share photos with your friends."
        }
      ],
      "expo-font",
      "expo-secure-store",
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app"
    ],
    "developmentClient": {
      "silentLaunch": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/dff077b6-4f6f-4c66-b07b-742deeca93a7"
    },
    "runtimeVersion": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/logo7.svg",
      "color": "#8B5CF6"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dff077b6-4f6f-4c66-b07b-742deeca93a7"
      }
    },
    "owner": "r.tomson"
  }
}
